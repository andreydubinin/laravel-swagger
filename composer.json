{
    "name": "andreydubinin/laravel-swagger",
    "description": "Auto generates the swagger documentation for a laravel project",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrey Dubinin",
            "email": "ald91@mail.ru"
        }
    ],
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "6.*"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Andreydubinin\\LaravelSwagger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Andreydubinin\\LaravelSwagger\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "ext-yaml": "Required to use the YAML output option"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Andreydubinin\\LaravelSwagger\\SwaggerServiceProvider"
            ]
        }
    }
}
